{
    "name": "slicks-slices-ui",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "gatsby develop",
        "build": "gatsby build",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "codegen:graphql": "gql-gen --config \"./graphql-codegen.yml\"",
        "check-all": "run-p check:*",
        "check:ts": "tsc --project ./tsconfig.json",
        "check:eslint": "eslint ./src",
        "check:stylelint": "stylelint ./src",
        "prettify": "prettier-eslint --write \"$PWD/src/**/*.{ts,tsx}\"",
        "prepare": "husky install",
        "release": "release-it --ci"
    },
    "dependencies": {
        "@hookform/resolvers": "2.8.3",
        "auto-changelog": "2.3.0",
        "gatsby": "4.1.0",
        "gatsby-plugin-image": "2.1.0",
        "gatsby-plugin-pnpm": "1.2.8",
        "gatsby-plugin-react-helmet": "5.1.0",
        "gatsby-plugin-root-import": "2.0.8",
        "gatsby-plugin-sass": "5.1.0",
        "gatsby-plugin-sharp": "4.1.0",
        "gatsby-source-sanity": "7.1.0",
        "gatsby-transformer-sharp": "4.1.0",
        "isomorphic-fetch": "3.0.0",
        "nodemailer": "6.7.0",
        "normalize.css": "8.0.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-helmet": "6.1.0",
        "react-hook-form": "7.18.1",
        "sass": "1.43.4",
        "waait": "1.0.5",
        "yup": "0.32.11"
    },
    "devDependencies": {
        "@babel/runtime": "7.16.0",
        "@graphql-codegen/cli": "2.2.2",
        "@graphql-codegen/typescript": "2.3.0",
        "@graphql-codegen/typescript-operations": "2.2.0",
        "@types/isomorphic-fetch": "0.0.35",
        "@types/nodemailer": "6.4.4",
        "@types/react-helmet": "6.1.4",
        "babel-loader": "8.2.3",
        "eslint": "8.1.0",
        "eslint-config-dva": "0.0.15",
        "graphql": "16.0.1",
        "husky": "7.0.4",
        "npm-run-all": "4.1.5",
        "prettier": "2.4.1",
        "prettier-eslint-cli": "5.0.1",
        "release-it": "14.11.6",
        "stylelint": "14.0.1",
        "stylelint-config-recess-order": "2.5.0",
        "stylelint-config-standard": "23.0.0",
        "stylelint-declaration-block-no-ignored-properties": "2.4.0",
        "stylelint-order": "5.0.0",
        "stylelint-scss": "4.0.0",
        "stylelint-selector-no-empty": "1.0.8",
        "stylelint-use-nesting": "3.0.0",
        "typescript": "4.4.4"
    }
}
